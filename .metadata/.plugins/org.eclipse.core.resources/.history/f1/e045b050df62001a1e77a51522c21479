package test;

import algorithm.FANormal;
import domain.ObjectiveFun;

public class FATest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 新建目标函数
		ObjectiveFun objectiveFun = new ObjectiveFun();
		FANormal faNormal = FANormal.builder().popNum(100).maxGen(100).dim(2).alpha(0.2).initAttraction(1.0)
				.gamma(0.33).isAdaptive(true).objectiveFun(objectiveFun).build();
		faNormal.start();
	}
}
