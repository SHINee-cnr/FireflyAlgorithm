package test;

import algorithm.FANormal;
import domain.ObjectiveFun;

public class FATest {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		// 新建目标函数
		ObjectiveFun objectiveFun = new ObjectiveFun();
		FANormal faNormal = FANormal.builder().popNum(100).maxGen(1000).dim(2).alpha(0.02).initAttraction(1.0).gamma(1.0)
				.objectiveFun(objectiveFun).build();
		faNormal.start();
	}
}
