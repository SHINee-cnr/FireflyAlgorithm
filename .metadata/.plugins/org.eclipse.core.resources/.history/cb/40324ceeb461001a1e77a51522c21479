package domain;

public class ObjectiveFun {
	public double objValue(double[] args) {
		double son = Math.sin(Math.sqrt((args[0] * args[0] + args[1] * args[1])));
		son = son * son - 0.5;
		double mot = 1 + 0.001 * (args[0] * args[0] + args[1] * args[1]);
		mot = mot * mot;
		return son / mot - 0.5;
	}
}
